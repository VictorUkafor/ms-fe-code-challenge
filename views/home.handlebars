<main>
			<div class="main-section">
				<div class="container">
						<div class="row">

							<div class="col-lg-12 col-md-12">

                                
									<div class="posts-section col-lg-4 col-md-4">
											<div class="post-topbar">
													<h2>@MakeSchool Tweets</h2>
												</div>
                                        {{#each tweets.MakeSchool}}
											<div class="post-bar">
												<div class="post_topbar">
													<div class="usy-dt">
														<img src={{user.profile_image_url}} alt="">
														<div class="usy-name">
															<h3>{{user.name}}</h3>
															<span><img src="images/clock.png" alt="">{{{trimDate created_at}}}</span>
														</div>
													</div>
												</div>
												<div class="job_descp">
													<a href={{entities.urls.[0].expanded_url}}><h3>{{text}}</h3></a>
												</div>
												<div class="job-status-bar">
													<ul class="like-com">
														<li><a href={{entities.urls.[0].expanded_url}}><i class="fa fa-retweet"></i> {{retweet_count}}</a></li> 
														<li><a href={{entities.urls.[0].expanded_url}}><i class="fa fa-heart"></i> {{favorite_count}}</a></li>
													</ul>
												</div>
											</div>
